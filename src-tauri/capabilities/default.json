{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "core:resources:default",
    "http:default",
    "fs:default",
    "shell:default",
    "geolocation:allow-check-permissions",
    "geolocation:allow-clear-permissions",
    "geolocation:allow-clear-watch",
    "geolocation:allow-get-current-position",
    "geolocation:allow-request-permissions",
    "geolocation:allow-watch-position",
    "blec:allow-check-permissions",
    "blec:allow-connect",
    "blec:allow-connection-state",
    "blec:allow-disconnect",
    "blec:allow-ping",
    "blec:allow-recv",
    "blec:allow-recv-string",
    "blec:allow-scan",
    "blec:allow-scanning-state",
    "blec:allow-send",
    "blec:allow-send-string",
    "blec:allow-stop-scan",
    "blec:allow-subscribe",
    "blec:allow-subscribe-string",
    "blec:allow-unsubscribe",
    "blec:default"
  ]
}